            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/zq_more_btn" />
    </RelativeLayout>

    <View

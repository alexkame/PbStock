<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_demand"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/re_6"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dim_100px"
        android:layout_marginTop="@dimen/trade_more_margin_top"
        android:background="@android:color/white"
        android:clickable="true"
        android:orientation="horizontal"
        android:visibility="visible" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/trade_margin_B"
            android:gravity="left"
            android:text="@string/IDS_BeiDuiSDJS"
            android:textColor="@color/gray_demand_one"
            android:textSize="@dimen/font_trade_more_B" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/trade_margin_B"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/chevron_default" />
    </RelativeLayout>
    
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.3dp"
        android:background="@color/gray_demand_one" 
         android:layout_marginLeft="@dimen/trade_margin_B"
        android:layout_marginRight="@dimen/trade_margin_B"/>
        
    <RelativeLayout
        android:id="@+id/re_1"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/trade_more_item_height"
        
        android:background="@android:color/white"
        android:clickable="true"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/trade_margin_B"
            android:gravity="left"
            android:text="@string/IDS_YinZhengZhuanZheng"
            android:textColor="@color/gray_demand_one"
            android:textSize="@dimen/font_trade_more_B" />

        <ImageView
            android:id="@+id/bank_security_choosebank_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/trade_margin_B"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/chevron_default" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.3dp"
        android:layout_marginLeft="@dimen/trade_margin_B"
        android:layout_marginRight="@dimen/trade_margin_B"
        android:background="@color/gray_demand_one" />

    <RelativeLayout
        android:id="@+id/re_2"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/trade_more_item_height"
        android:background="@android:color/white"
        android:clickable="true"
        android:orientation="horizontal"
        android:visibility="visible" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/trade_margin_B"
            android:gravity="left"
            android:text="@string/IDS_XiuGaiMiMa"
            android:textColor="@color/gray_demand_one"
            android:textSize="@dimen/font_trade_more_B" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/trade_margin_B"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/chevron_default" />
    </RelativeLayout>

    <View
        android:id="@+id/view_re3"
        android:layout_width="fill_parent"
        android:layout_height="0.3dp"
        android:background="@color/gray_demand_one" 
         android:layout_marginLeft="@dimen/trade_margin_B"
        android:layout_marginRight="@dimen/trade_margin_B"/>

    <RelativeLayout
        android:id="@+id/re_3"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dim_100px"
        android:background="@android:color/white"
        android:clickable="true"
        android:orientation="horizontal"
        android:visibility="visible" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/trade_margin_B"
            android:gravity="left"
            android:text="@string/IDS_XiuGaiRenZhengKouLing"
            android:textColor="@color/gray_demand_one"
            android:textSize="@dimen/font_trade_more_B" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/trade_margin_B"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/chevron_default" />
    </RelativeLayout>
    
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.3dp"
        android:background="@color/gray_demand_one" 
         android:layout_marginLeft="@dimen/trade_margin_B"
        android:layout_marginRight="@dimen/trade_margin_B"/>

    <RelativeLayout
        android:id="@+id/re_4"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dim_100px"
        android:background="@android:color/white"
        android:clickable="true"
        android:orientation="horizontal"
        android:visibility="visible" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/trade_margin_B"
            android:gravity="left"
            android:text="@string/IDS_LiShiWeiTuo"
            android:textColor="@color/gray_demand_one"
            android:textSize="@dimen/font_trade_more_B" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/trade_margin_B"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/chevron_default" />
    </RelativeLayout>
    
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.3dp"
        android:background="@color/gray_demand_one" 
         android:layout_marginLeft="@dimen/trade_margin_B"
        android:layout_marginRight="@dimen/trade_margin_B"/>

    <RelativeLayout
        android:id="@+id/re_5"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dim_100px"
        android:background="@android:color/white"
        android:clickable="true"
        android:orientation="horizontal"
        android:visibility="visible" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/trade_margin_B"
            android:gravity="left"
            android:text="@string/IDS_LiShiChengJiao"
            android:textColor="@color/gray_demand_one"
            android:textSize="@dimen/font_trade_more_B" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/trade_margin_B"
            android:contentDescription="@string/IDS_TuPianMiaoShu"
            android:src="@drawable/chevron_default" />
    </RelativeLayout>
    


    

    <Button
        android:id="@+id/exit_account"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         android:layout_marginLeft="@dimen/trade_margin_B"
        android:layout_marginRight="@dimen/trade_margin_B"
        android:layout_marginTop="@dimen/trade_margin_B"
        android:background="@drawable/all_red_btn_press_selector"
        android:text="@string/IDS_TuiChuZhangHu"
        android:textColor="@android:color/white" 
        android:textSize="@dimen/font_trade_more_A"/>

</LinearLayout>